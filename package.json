{
  "name": "fuzzy-dbscan",
  "version": "0.0.1",
  "description": "Fuzzy DBSCAN algorithm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "bundle": "browserify dist/fuzzy-dbscan.js --standalone FuzzyDBSCAN --outfile fuzzy-dbscan.js",
    "prepublish": "npm run build",
    "test": "mocha --require ts-node/register test/*",
    "test:trace": "mocha --require ts-node/register --trace-deopt --trace-opt test/* > trace.log"
  },
  "keywords": [
    "fuzzy",
    "cluster",
    "clustering",
    "FuzzyDBSCAN",
    "DBSCAN",
    "algorithm"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "browserify": "^15.2.0",
    "chai": "^4.1.2",
    "marked": "^0.3.6",
    "mocha": "^5.0.0",
    "pureimage": "^0.1.3",
    "seed-random": "^2.2.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "author": "Christoph Schulz <schulzcbs@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/schulzch/fuzzy-dbscan-js",
  "bugs": {
    "url": "https://github.com/schulzch/fuzzy-dbscan-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/schulzch/fuzzy-dbscan-js.git"
  }
